
public class Act8 {
	public static void main(String args[]) {

		int number1;
		int number2;
		double sum;
		double sub;
		double m;
		double rem;
		float div,number11 ,number22;
		
		number1 = 10;
		number2 = 20;
		number11 = 10.9f;
		number22 = 20.3f;
		
		sum = number1 + number2;
		sub = number1 - number2;
		m = number1 * number2;
		div =  number11 / number22;
		
		rem = number1 % number2;
		/*
		System.out.println("The addition is: " + sum);
		System.out.println("The subtract is: " + sub);
		System.out.println("The multipluication is: " + m);
		System.out.println("The division is: " + div);
		System.out.println("The remainder is: " + rem);
		*/
		
		System.out.print("The addition is: " + sum + "\nThe subtract is: " + sub + "\nThe multipluication is: " + m + "\nThe division is: " + div +  "\nThe remainder is: " + rem);
	}
}


